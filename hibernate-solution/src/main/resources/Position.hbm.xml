<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="by.htp.basumatarau.hibernate.dao.beans.Position" table="job_positions">

		<meta attribute="class-description">
			This class contains the job position detail.
		</meta>
		<id name="id" type="int" column="id_job_position">
			<generator class="native" />
		</id>
		<property name="name" column="position" type="string" />

	</class>
</hibernate-mapping>